<Window x:Class="project_e.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:project_e"
        mc:Ignorable="d"
        Title="MainWindow" Height="482" Width="831"
        KeyDown="Window_KeyDown">
    <Grid>
        <Button x:Name="Generate" Content="Generate" HorizontalAlignment="Left" Margin="715,404,0,0" VerticalAlignment="Top" Width="75" Click="Generate_Click"/>
        <Button x:Name="ImportFile" Content="Import" HorizontalAlignment="Left" Margin="10,404,0,0" VerticalAlignment="Top" Width="75" Click="ImportFileButton_Click"/>
        <ComboBox x:Name="NumberOfBars" HorizontalAlignment="Left" Margin="630,351,0,0" VerticalAlignment="Top" Width="75"/>
        <Label Content="# of Bars" HorizontalAlignment="Left" Margin="639,320,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="Tempo" HorizontalAlignment="Left" Margin="630,404,0,0" TextWrapping="Wrap" Text="100" VerticalAlignment="Top" Width="75"/>
        <Label  Content="Tempo" HorizontalAlignment="Left" Margin="645,378,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="NumberOfNotes" HorizontalAlignment="Left" Margin="713,350,0,0" VerticalAlignment="Top" Width="75"/>
        <Label Content="# of Notes" HorizontalAlignment="Left" Margin="717,319,0,0" VerticalAlignment="Top"/>
        <ListBox x:Name="BassNotes" HorizontalAlignment="Left" Height="100" Margin="360,135,0,0" VerticalAlignment="Top" Width="100"/>
        <ListBox x:Name="Intervals" HorizontalAlignment="Left" Height="250" Margin="723,60,0,0" VerticalAlignment="Top" Width="65"/>
        <Label Content="Intervals" HorizontalAlignment="Left" Margin="729,26,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
