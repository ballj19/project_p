        <Label Content="Biggest Hop" HorizontalAlignment="Left" Margin="715,320,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="NumberOfBars" HorizontalAlignment="Left" Margin="630,351,0,0" VerticalAlignment="Top" Width="75"/>
        <Label Content="# of Bars" HorizontalAlignment="Left" Margin="639,320,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="Tempo" HorizontalAlignment="Left" Margin="630,404,0,0" TextWrapping="Wrap" Text="100" VerticalAlignment="Top" Width="75"/>
        <Label  Content="Tempo" HorizontalAlignment="Left" Margin="645,378,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
